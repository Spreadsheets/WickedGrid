/*navigation starts*/
.nav {width: 100%;height: auto;float: left; height: 23px;background-color: #e9e9e9;}

#menu, #menu ul {
	margin: 0 auto;
	padding: 0;
}

#menu {
	display: inline-block;
	min-width: 50%;
	list-style: none;
	/*border-top: 1px solid #ccc;border-left: 1px solid #ccc;border-bottom: 1px solid #ccc;*/
	background-color: #e9e9e9;
	font-family: Arial, verdana;
	font-size: 12px;
	color: #3a3a3a;
	height: 23px;
}

#menu>li {
	float: left;
	position: relative;
	list-style: none;
	border-right: 1px solid #a3a2a2;
	border-left: 1px solid #fff;
}

#menu li {
	float: left;
	position: relative;
	list-style: none;
}

#menu>li:hover>ul {
	display: block;
}

#menu>li>ul {
	display: none;
	position: absolute;
}

#menu li a {
	display: block;
	padding: 4px 10px;
	color: #3a3a3a;
}

#menu li a {
	display: block;
	padding: 4px 10px;
	text-decoration: none;
	font-weight: normal;
	white-space: nowrap;
}

#menu li a:hover {
	color: #fff;
	background: #5fa364;
}

.highlight {
	background-color: #5fa364;
	color: #ffffff !important;
}

