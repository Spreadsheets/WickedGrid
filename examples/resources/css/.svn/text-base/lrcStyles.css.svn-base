/*perzonlization-menu*/
ul#pmenu{background: inherit; border:none;}
ul#pmenu li {}
ul#pmenu li a{width:auto; color:#ffffff; text-decoration:none; padding: 5px 15px 5px 5px;}

#pmenu, #pmenu ul {
	
	margin: 0;
	padding: 0;
	list-style: none; 
	color: #336699;
	background: #e1dede;
	border:solid 1px #c2c2c2;
	
}

#pmenu, #pmenu ul li {
	display: block;
	position: relative;
	float: left;
	width:90px;
	z-index:100;
}

#pmenu li ul {
	display: none;
	top:15px;
	-webkit-box-shadow: -1px 10px 15px 0px rgba(211,211,210,0.75);
	-moz-box-shadow: -1px 10px 15px 0px rgba(211,211,210,0.75);
	box-shadow: -1px 10px 15px 0px rgba(211,211,210,0.75);

}

#pmenu, #pmenu ul li a {
	display: block;
	text-decoration: none !important;
	color:  #000000 !important;	
	background: #e1dede;
	margin-left: 1px;
	white-space: nowrap;
	height:15px;
	text-align:right;
}

#pmenu, #pmenu ul li a:hover { background: #f9f8f8;}

#pmenu, #pmenu li:hover ul {
	display: block;
	position: absolute;
}

#pmenu, #pmenu li:hover li {
	float: none;
	font-size: 11px;
}

#pmenu, #pmenu li:hover a { /*background: #3b3b3b;*/
	
}

#pmenu, #pmenu li:hover li a:hover {	
	background-color: #f9f8f8;
	color: #000000 !important;
	border-top: 0px solid #ffffff;
}

