/*Grid*/
#testcss .ui-jqgrid {position: relative;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-view {position: relative;left:0; top: 0; padding: 0; font-size:11px;}
/* caption*/
#testcss .ui-jqgrid #testcss .ui-jqgrid-titlebar {padding: .3em .2em .2em .3em; position: relative; font-size: 12px; border-left: 0 none;border-right: 0 none; border-top: 0 none;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-caption {text-align: left;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-title { margin: .1em 0 .2em; }
#testcss .ui-jqgrid #testcss .ui-jqgrid-titlebar-close { position: absolute;top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height:18px; cursor:pointer;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-titlebar-close span { display: block; margin: 1px; }
#testcss .ui-jqgrid #testcss .ui-jqgrid-titlebar-close:hover { padding: 0; }
/* header*/
#testcss .ui-jqgrid #testcss .ui-jqgrid-hdiv {position: relative; margin: 0;padding: 0; overflow-x: hidden; border-left: 0 none !important; border-top : 0 none !important; border-right : 0 none !important;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-hbox {float: left; padding-right: 20px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-htable {table-layout:fixed;margin:0;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-htable th {height:22px;padding: 0 2px 0 2px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-htable th div {overflow: hidden; position:relative; height:17px;}
#testcss .ui-th-column, #testcss .ui-jqgrid #testcss .ui-jqgrid-htable th#testcss .ui-th-column {overflow: hidden;white-space: nowrap;text-align:center;border-top : 0 none;border-bottom : 0 none;}
#testcss .ui-th-ltr, #testcss .ui-jqgrid #testcss .ui-jqgrid-htable th#testcss .ui-th-ltr {border-left : 0 none;}
#testcss .ui-th-rtl, #testcss .ui-jqgrid #testcss .ui-jqgrid-htable th#testcss .ui-th-rtl {border-right : 0 none;}
#testcss .ui-first-th-ltr {border-right: 1px solid; }
#testcss .ui-first-th-rtl {border-left: 1px solid; }
#testcss .ui-jqgrid #testcss .ui-th-div-ie {white-space: nowrap; zoom :1; height:17px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-resize {height:20px !important;position: relative; cursor :e-resize;display: inline;overflow: hidden;}
#testcss .ui-jqgrid #testcss .ui-grid-ico-sort {overflow:hidden;position:absolute;display:inline; cursor: pointer !important;}
#testcss .ui-jqgrid #testcss .ui-icon-asc {margin-top:-3px; height:12px;}
#testcss .ui-jqgrid #testcss .ui-icon-desc {margin-top:3px;height:12px;}
#testcss .ui-jqgrid #testcss .ui-i-asc {margin-top:0;height:16px;}
#testcss .ui-jqgrid #testcss .ui-i-desc {margin-top:0;margin-left:13px;height:16px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-sortable {cursor:pointer;}
#testcss .ui-jqgrid tr#testcss .ui-search-toolbar th { border-top-width: 1px !important; border-top-color: inherit !important; border-top-style: ridge !important }
tr#testcss .ui-search-toolbar input {margin: 1px 0 0 0}
tr#testcss .ui-search-toolbar select {margin: 1px 0 0 0}
/* body */ 
#testcss .ui-jqgrid #testcss .ui-jqgrid-bdiv {position: relative; margin: 0; padding:0; overflow: auto; text-align:left;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-btable {table-layout:fixed; margin:0; outline-style: none; }
#testcss .ui-jqgrid tr.jqgrow { outline-style: none; }
#testcss .ui-jqgrid tr.jqgroup { outline-style: none; }
#testcss .ui-jqgrid tr.jqgrow td {font-weight: normal; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
#testcss .ui-jqgrid tr.jqgfirstrow td {padding: 0 2px 0 2px;border-right-width: 1px; border-right-style: solid;}
#testcss .ui-jqgrid tr.jqgroup td {font-weight: normal; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
#testcss .ui-jqgrid tr.jqfoot td {font-weight: bold; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
#testcss .ui-jqgrid tr#testcss .ui-row-ltr td {text-align:left;border-right-width: 1px; border-right-color: inherit; border-right-style: solid;}
#testcss .ui-jqgrid tr#testcss .ui-row-rtl td {text-align:right;border-left-width: 1px; border-left-color: inherit; border-left-style: solid;}
#testcss .ui-jqgrid td.jqgrid-rownum { padding: 0 2px 0 2px; margin: 0; border: 0 none;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-resize-mark { width:2px; left:0; background-color:#777; cursor: e-resize; cursor: col-resize; position:absolute; top:0; height:100px; overflow:hidden; display:none; border:0 none; z-index: 99999;}
/* footer */
#testcss .ui-jqgrid #testcss .ui-jqgrid-sdiv {position: relative; margin: 0;padding: 0; overflow: hidden; border-left: 0 none !important; border-top : 0 none !important; border-right : 0 none !important;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-ftable {table-layout:fixed; margin-bottom:0;}
#testcss .ui-jqgrid tr.footrow td {font-weight: bold; overflow: hidden; white-space:nowrap; height: 21px;padding: 0 2px 0 2px;border-top-width: 1px; border-top-color: inherit; border-top-style: solid;}
#testcss .ui-jqgrid tr.footrow-ltr td {text-align:left;border-right-width: 1px; border-right-color: inherit; border-right-style: solid;}
#testcss .ui-jqgrid tr.footrow-rtl td {text-align:right;border-left-width: 1px; border-left-color: inherit; border-left-style: solid;}
/* Pager*/
#testcss .ui-jqgrid #testcss .ui-jqgrid-pager { border-left: 0 none !important;border-right: 0 none !important; border-bottom: 0 none !important; margin: 0 !important; padding: 0 !important; position: relative; height: 25px;white-space: nowrap;overflow: hidden;font-size:11px;}
#testcss .ui-jqgrid #testcss .ui-pager-control {position: relative;}
#testcss .ui-jqgrid #testcss .ui-pg-table {position: relative; padding-bottom:2px; width:auto; margin: 0;}
#testcss .ui-jqgrid #testcss .ui-pg-table td {font-weight:normal; vertical-align:middle; padding:1px;}
#testcss .ui-jqgrid #testcss .ui-pg-button  { height:19px !important;}
#testcss .ui-jqgrid #testcss .ui-pg-button span { display: block; margin: 1px; float:left;}
#testcss .ui-jqgrid #testcss .ui-pg-button:hover { padding: 0; }
#testcss .ui-jqgrid #testcss .ui-state-disabled:hover {padding:1px;}
#testcss .ui-jqgrid #testcss .ui-pg-input { height:13px;font-size:.8em; margin: 0;}
#testcss .ui-jqgrid #testcss .ui-pg-selbox {font-size:.8em; line-height:18px; display:block; height:18px; margin: 0;}
#testcss .ui-jqgrid #testcss .ui-separator {height: 18px; border-left: 1px solid #ccc ; border-right: 1px solid #ccc ; margin: 1px; float: right;}
#testcss .ui-jqgrid #testcss .ui-paging-info {font-weight: normal;height:19px; margin-top:3px;margin-right:4px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-pager #testcss .ui-pg-div {padding:1px 0;float:left;position:relative;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-pager #testcss .ui-pg-button { cursor:pointer; }
#testcss .ui-jqgrid #testcss .ui-jqgrid-pager #testcss .ui-pg-div  span#testcss .ui-icon {float:left;margin:0 2px;}
#testcss .ui-jqgrid td input, #testcss .ui-jqgrid td select #testcss .ui-jqgrid td textarea { margin: 0;}
#testcss .ui-jqgrid td textarea {width:auto;height:auto;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-toppager {border-left: 0 none !important;border-right: 0 none !important; border-top: 0 none !important; margin: 0 !important; padding: 0 !important; position: relative; height: 25px !important;white-space: nowrap;overflow: hidden;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-toppager #testcss .ui-pg-div {padding:1px 0;float:left;position:relative;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-toppager #testcss .ui-pg-button { cursor:pointer; }
#testcss .ui-jqgrid #testcss .ui-jqgrid-toppager #testcss .ui-pg-div  span#testcss .ui-icon {float:left;margin:0 2px;}
/*subgrid*/
#testcss .ui-jqgrid #testcss .ui-jqgrid-btable #testcss .ui-sgcollapsed span {display: block;}
#testcss .ui-jqgrid #testcss .ui-subgrid {margin:0;padding:0; width:100%;}
#testcss .ui-jqgrid #testcss .ui-subgrid table {table-layout: fixed;}
#testcss .ui-jqgrid #testcss .ui-subgrid tr#testcss .ui-subtblcell td {height:18px;border-right-width: 1px; border-right-color: inherit; border-right-style: solid;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
#testcss .ui-jqgrid #testcss .ui-subgrid td.subgrid-data {border-top:  0 none !important;}
#testcss .ui-jqgrid #testcss .ui-subgrid td.subgrid-cell {border-width: 0 0 1px 0;}
#testcss .ui-jqgrid #testcss .ui-th-subgrid {height:20px;}
/* loading */
#testcss .ui-jqgrid .loading {position: absolute; top: 45%;left: 45%;width: auto;z-index:101;padding: 6px; margin: 5px;text-align: center;font-weight: bold;display: none;border-width: 2px !important; font-size:11px;}
#testcss .ui-jqgrid .jqgrid-overlay {display:none;z-index:100;}
/* IE * html .jqgrid-overlay {width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');} */
* .jqgrid-overlay iframe {position:absolute;top:0;left:0;z-index:-1;}
/* IE width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');}*/
/* end loading div */
/* toolbar */
#testcss .ui-jqgrid #testcss .ui-userdata {border-left: 0 none;    border-right: 0 none;	height : 21px;overflow: hidden;	}
/*Modal Window */
#testcss .ui-jqdialog { display: none; width: 300px; position: absolute; padding: .2em; font-size:11px; overflow:visible;}
#testcss .ui-jqdialog #testcss .ui-jqdialog-titlebar { padding: .3em .2em; position: relative;  }
#testcss .ui-jqdialog #testcss .ui-jqdialog-title { margin: .1em 0 .2em; } 
#testcss .ui-jqdialog #testcss .ui-jqdialog-titlebar-close { position: absolute;  top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height: 18px; cursor:pointer;}

#testcss .ui-jqdialog #testcss .ui-jqdialog-titlebar-close span { display: block; margin: 1px; }
#testcss .ui-jqdialog #testcss .ui-jqdialog-titlebar-close:hover, #testcss .ui-jqdialog #testcss .ui-jqdialog-titlebar-close:focus { padding: 0; }
#testcss .ui-jqdialog-content, #testcss .ui-jqdialog #testcss .ui-jqdialog-content { border: 0; padding: .3em .2em; background: none; height:auto;}
#testcss .ui-jqdialog #testcss .ui-jqconfirm {padding: .4em 1em; border-width:3px;position:absolute;bottom:10px;right:10px;overflow:visible;display:none;height:80px;width:220px;text-align:center;}
#testcss .ui-jqdialog>#testcss .ui-resizable-se { bottom: -3px; right: -3px}
#testcss .ui-jqgrid>#testcss .ui-resizable-se { bottom: -3px; right: -3px }
/* end Modal window*/
/* Form edit */
#testcss .ui-jqdialog-content .FormGrid {margin: 0;}
#testcss .ui-jqdialog-content .EditTable { width: 100%; margin-bottom:0;}
#testcss .ui-jqdialog-content .DelTable { width: 100%; margin-bottom:0;}
.EditTable td input, .EditTable td select, .EditTable td textarea {margin: 0;}
.EditTable td textarea { width:auto; height:auto;}
#testcss .ui-jqdialog-content td.EditButton {text-align: right;border-top: 0 none;border-left: 0 none;border-right: 0 none; padding-bottom:5px; padding-top:5px;}
#testcss .ui-jqdialog-content td.navButton {text-align: center; border-left: 0 none;border-top: 0 none;border-right: 0 none; padding-bottom:5px; padding-top:5px;}
#testcss .ui-jqdialog-content input.FormElement {padding:.3em}
#testcss .ui-jqdialog-content select.FormElement {padding:.3em}
#testcss .ui-jqdialog-content .data-line {padding-top:.1em;border: 0 none;}

#testcss .ui-jqdialog-content .CaptionTD {vertical-align: middle;border: 0 none; padding: 2px;white-space: nowrap;}
#testcss .ui-jqdialog-content .DataTD {padding: 2px; border: 0 none; vertical-align: top;}
#testcss .ui-jqdialog-content .form-view-data {white-space:pre}
.fm-button { display: inline-block; margin:0 4px 0 0; padding: .4em .5em; text-decoration:none !important; cursor:pointer; position: relative; text-align: center; zoom: 1; }
.fm-button-icon-left { padding-left: 1.9em; }
.fm-button-icon-right { padding-right: 1.9em; }
.fm-button-icon-left #testcss .ui-icon { right: auto; left: .2em; margin-left: 0; position: absolute; top: 50%; margin-top: -8px; }
.fm-button-icon-right #testcss .ui-icon { left: auto; right: .2em; margin-left: 0; position: absolute; top: 50%; margin-top: -8px;}
#nData, #pData { float: left; margin:3px;padding: 0; width: 15px; }
/* End Eorm edit */
/*#testcss .ui-jqgrid .edit-cell {}*/
#testcss .ui-jqgrid .selected-row, div#testcss .ui-jqgrid .selected-row td {font-style : normal;border-left: 0 none;}
/* inline edit actions button*/
#testcss .ui-inline-del#testcss .ui-state-hover span, #testcss .ui-inline-edit#testcss .ui-state-hover span,
#testcss .ui-inline-save#testcss .ui-state-hover span, #testcss .ui-inline-cancel#testcss .ui-state-hover span {
    margin: -1px;
}
/* Tree Grid */
#testcss .ui-jqgrid .tree-wrap {float: left; position: relative;height: 18px;white-space: nowrap;overflow: hidden;}
#testcss .ui-jqgrid .tree-minus {position: absolute; height: 18px; width: 18px; overflow: hidden;}
#testcss .ui-jqgrid .tree-plus {position: absolute;	height: 18px; width: 18px;	overflow: hidden;}
#testcss .ui-jqgrid .tree-leaf {position: absolute;	height: 18px; width: 18px;overflow: hidden;}
#testcss .ui-jqgrid .treeclick {cursor: pointer;}
/* moda dialog */
* iframe.jqm {position:absolute;top:0;left:0;z-index:-1;}
/*	 width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');}*/
#testcss .ui-jqgrid-dnd tr td {border-right-width: 1px; border-right-color: inherit; border-right-style: solid; height:20px}
/* RTL Support */
#testcss .ui-jqgrid #testcss .ui-jqgrid-caption-rtl {text-align: right;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-hbox-rtl {float: right; padding-left: 20px;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-resize-ltr {float: right;margin: -2px -2px -2px 0;}
#testcss .ui-jqgrid #testcss .ui-jqgrid-resize-rtl {float: left;margin: -2px 0 -1px -3px;}
#testcss .ui-jqgrid #testcss .ui-sort-rtl {left:0;}
#testcss .ui-jqgrid .tree-wrap-ltr {float: left;}
#testcss .ui-jqgrid .tree-wrap-rtl {float: right;}
#testcss .ui-jqgrid #testcss .ui-ellipsis {-moz-text-overflow:ellipsis;text-overflow:ellipsis;}

/* Toolbar Search Menu */
#testcss .ui-search-menu { position: absolute; padding: 2px 5px;}
#testcss .ui-jqgrid #testcss .ui-search-table { padding: 0; border: 0 none; height:20px; width:100%;}
#testcss .ui-jqgrid #testcss .ui-search-table #testcss .ui-search-oper { width:20px; }
a.g-menu-item, a.soptclass, a.clearsearchclass { cursor: pointer; }
#testcss .ui-jqgrid #testcss .ui-search-table #testcss .ui-search-input>input,
#testcss .ui-jqgrid #testcss .ui-search-table #testcss .ui-search-input>select
{
    display: block;
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
} 
#testcss .ui-jqgrid #testcss .ui-jqgrid-view input,
#testcss .ui-jqgrid #testcss .ui-jqgrid-view select,
#testcss .ui-jqgrid #testcss .ui-jqgrid-view textarea,
#testcss .ui-jqgrid #testcss .ui-jqgrid-view button {
    font-size: 11px
}
