/*serach*/
.adv-search-header-container {
	width: 100%;
	height: 36px;
	float: left;
	background-color: #fff;
}

.adv-search-header {
	width: auto;
	height: auto;
	float: left;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 16px;
}

.arrow-d {
	width: 8px;
	height: 4px;
	float: left;
	margin: 0px 0px 0px 10px;
	border: #f00 0px solid;
}

.searchbox {
	width: 100%;
	float: left;
	background-color: #f9f9f9;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #fff;
	padding-bottom: 10px;
}

.textalign-r {
	text-align: right;
	text-indent: 3px;
}

.search-labels-box {
	width: 100%;
	float: left;
	margin: 10px 0px -8px 25px;
	float: left;
	border: 0px solid #f00;
}

.srch-label {
	width: 16.5%;
	float: left;
	height:19px;
}

.srch-text-box-row {
	width: 100%;
	float: left;
	margin: 5px 0px 0px 25px;
	float: left;
	border: 0px solid #f00;
}

.srch-result-count { /* width:100%; */
	font-style: normal;
	float: left;
	background-color: #f3f3f3;
	color: #5FB404;
	padding-left: 3px;
	padding-top:5px;
	font-family: Segoe UI;
	font-size: 13px;
	text-align: left;
}

.srch-result-box {
	width: 100%;
	height: auto;
	float: left;
	overflow: auto;
	margin: 10px 0px 10px 0px; 
}

.rangeFieldDiv {
	position: relative;
	display: inline-block;
	cursor: default;
	text-align: left;
	line-height: 20px;
	clear: both;
	color: #888;
	font-size: 11px;
	font-family: Segoe UI;
	margin-right: 2px;
}

.rangeInput {
	width: 88px;
	height: 19px;
	text-indent: 5px; /*  float:left; */
	color: #888;
	border-radius: 0px;
	font-size: 11px;
	font-family: Segoe UI;
	border: 1px solid #1d8acf;
}

.fieldInput {
	width: 185px;
	height: 19px;
	text-indent: 5px; /* float:left; */
	color: #888;
	border-radius: 0px;
	font-size: 11px;
	font-family: Segoe UI;
	border: 1px solid #1d8acf;
}

.closeSearchBox {
	text-decoration: none;
	font-size: 15px;
}

/*select*/
div.selectBox {
	position: relative;
	display: inline-block;
	cursor: default;
	text-align: left;
	clear: both;
	color: #888;
	font-size: 11px;
	font-family: Segoe UI;
	margin-right: 2px;
}

span.selected {
	width: 67px;
	text-indent: 5px;
	color: #808080;
	border: 1px solid #1d8acf;
	border-right: 0px solid #1d8acf;
	border-top-left-radius: 0px;
	border-bottom-left-radius: 0px;
	background: #fff;
	overflow: hidden;
}

span.selectArrow {
	width: 30px;
	border-left: 0px solid #fff;
	border-top: 1px solid #1d8acf;
	border-right: 1px solid #1d8acf;
	border-bottom: 1px solid #1d8acf;
	border-top-right-radius: 0px;
	border-bottom-right-radius: 0px;
	text-align: center;
	font-size: 10px;
	font-family: Segoe UI;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-o-user-select: none;
	user-select: none; /*background:#4096ee;*/
	background: #fff;
}

span.selectArrow,span.selected {
	position: relative;
	float: left;
	/*height: 20px;*/
	z-index: 1;
}

div.selectOptions {
    background: none repeat scroll 0 0 #f6f6f6;
    border: 1px solid #ccc;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0;
    display: none;
    left: 0;
    max-height: 220px;
    overflow: scroll;
    padding-top: 2px;
    position: absolute;
    top: 20px;
    width: 99px;
    z-index: 10;
}

span.selectOption {
    color: #4096ee;
    display: block;
    font-family: Segoe UI;
    font-size: 11px;
    line-height: 20px;
    padding: 1px 2%;
    white-space: nowrap;
    width: 100%;
}

span.selectOption:hover {
	color: black;
}
/*select*/

/* Error */
.errorDiv {
	color: red;
	display: inherit;
	float: left;
	font-family: Segoe UI;
	font-style: oblique;
	height: 15px;
	margin-bottom: 0;
	padding-left: 15px;
	padding-top: 2px;
	text-align: left;
}

.errorSpan {
	padding: 3px;
	height: 15px;
	width: 100%;
}
/* Error */

/*search*/
.fieldSet {
	border: 1px solid #c8c8c8;
    margin-bottom: 5px;
    margin-left: 15px;
    margin-right: 15px;
    padding-bottom: 10px;
}