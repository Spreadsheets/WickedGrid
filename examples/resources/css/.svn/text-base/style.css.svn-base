	/*select*/
	div.selectBox{position:relative;display:inline-block;cursor:default; text-align:left; line-height:20px; clear:both;	color:#888; font-size:11px;font-family:Segoe UI; margin-right:2px;}
	span.selected{width:67px;text-indent:5px;	border:1px solid #1d8acf; border-right:0px solid #1d8acf; border-top-left-radius:3px; border-bottom-left-radius:3px; background:#fff; overflow:hidden;}
	span.selectArrow{width:30px; border-left:1px solid #fff; border-top:1px solid #1d8acf;	border-right:1px solid #1d8acf; border-bottom:1px solid #1d8acf; border-top-right-radius:3px; border-bottom-right-radius:3px; text-align:center;font-size:10px;font-family:Segoe UI; -webkit-user-select: none;-khtml-user-select: none;-moz-user-select: none; -o-user-select: none; user-select: none; /*background:#4096ee;*/ background:#fff;}
	span.selectArrow,span.selected{position:relative;float:left;height:20px;z-index:1;}
	div.selectOptions{position:absolute;top:20px;left:0;width:99px;border:1px solid #ccc;border-bottom-right-radius:0px;border-bottom-left-radius:0px;overflow:hidden;background:#f6f6f6;padding-top:2px;display:none;}
	span.selectOption{display:block;width:80%;line-height:20px;padding:5px 2%; font-size:11px;font-family:Segoe UI;color:#4096ee;}
	span.selectOption:hover{color:#4096ee;}	
